{
    "name": "blockadelabs/sdk",
    "description": "Blockade Labs SDK for Laravel",
    "keywords": [
        "blockade labs",
        "sdk",
        "laravel"
    ],
    "license": "MIT",
    "homepage": "https://github.com/blockade/sdk",
    "type": "library",
    "require": {
        "php": "^7.25|^8.0",
        "illuminate/support": "^7.0|^8.0",
        "guzzlehttp/guzzle": "^6.3.1|^7.0.1"
    },
    "autoload": {
        "psr-4": {
            "BlockadeLabs\\SDK\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BlockadeLabs\\SDK\\SDKServiceProvider"
            ],
            "aliases": {
                "SDK": "BlockadeLabs\\SDK\\SDKFacade"
            }
        }
    }
}
